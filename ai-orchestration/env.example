# AI Orchestration Service Environment Variables
# Copy this file to .env for local development

# Database
DATABASE_URL=postgresql://ledgerflow:ledgerflow_dev@postgres:5432/ledgerflow

# ChromaDB
CHROMADB_HOST=chromadb
CHROMADB_PORT=8000

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# AI Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET_NAME=ledgerflow-documents
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789/invoice-processing

# Application
ENVIRONMENT=development
LOG_LEVEL=INFO



